#! /usr/bin/vvp
:ivl_version "10.2 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision - 12;
:vpi_module "system";
:vpi_module "vhdl_sys";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_0xca0710 .scope module, "Decode24Test_v" "Decode24Test_v" 2 26;
 .timescale -9 -12;
v0xcbd040_0 .var "in", 1 0;
v0xcbd130_0 .net "out", 3 0, v0xcbcf00_0;  1 drivers
v0xcbd200_0 .var "passed", 7 0;
S_0xca0890 .scope task, "allPassed" "allPassed" 2 43, 2 43 0, S_0xca0710;
 .timescale -9 -12;
v0xca2860_0 .var "numTests", 7 0;
v0xcbc4e0_0 .var "passed", 7 0;
TD_Decode24Test_v.allPassed ;
    %load/vec4 v0xcbc4e0_0;
    %load/vec4 v0xca2860_0;
    %cmp/e;
    %jmp/0xz  T_0.0, 4;
    %vpi_call 2 47 "$display", "All tests passed" {0 0 0};
    %jmp T_0.1;
T_0.0 ;
    %vpi_call 2 48 "$display", "Some tests failed" {0 0 0};
T_0.1 ;
    %end;
S_0xcbc5c0 .scope task, "passTest" "passTest" 2 34, 2 34 0, S_0xca0710;
 .timescale -9 -12;
v0xcbc7b0_0 .var "actualOut", 3 0;
v0xcbc890_0 .var "expectedOut", 3 0;
v0xcbc970_0 .var "passed", 7 0;
v0xcbca60_0 .var "testType", 120 0;
TD_Decode24Test_v.passTest ;
    %load/vec4 v0xcbc7b0_0;
    %load/vec4 v0xcbc890_0;
    %cmp/e;
    %jmp/0xz  T_1.2, 4;
    %vpi_call 2 39 "$display", "%s passed", v0xcbca60_0 {0 0 0};
    %load/vec4 v0xcbc970_0;
    %addi 1, 0, 8;
    %store/vec4 v0xcbc970_0, 0, 8;
    %jmp T_1.3;
T_1.2 ;
    %vpi_call 2 40 "$display", "%s failed: %d should be %d", v0xcbca60_0, v0xcbc7b0_0, v0xcbc890_0 {0 0 0};
T_1.3 ;
    %end;
S_0xcbcb40 .scope module, "uut" "Decode24" 2 59, 3 1 0, S_0xca0710;
 .timescale 0 0;
    .port_info 0 /OUTPUT 4 "out"
    .port_info 1 /INPUT 2 "in"
v0xcbce00_0 .net "in", 1 0, v0xcbd040_0;  1 drivers
v0xcbcf00_0 .var "out", 3 0;
E_0xcbcd80 .event edge, v0xcbce00_0;
    .scope S_0xcbcb40;
T_2 ;
    %wait E_0xcbcd80;
    %load/vec4 v0xcbce00_0;
    %parti/s 1, 0, 2;
    %inv;
    %load/vec4 v0xcbce00_0;
    %parti/s 1, 1, 2;
    %inv;
    %and;
    %ix/load 4, 0, 0;
    %flag_set/imm 4, 0;
    %store/vec4 v0xcbcf00_0, 4, 1;
    %load/vec4 v0xcbce00_0;
    %parti/s 1, 0, 2;
    %load/vec4 v0xcbce00_0;
    %parti/s 1, 1, 2;
    %inv;
    %and;
    %ix/load 4, 1, 0;
    %flag_set/imm 4, 0;
    %store/vec4 v0xcbcf00_0, 4, 1;
    %load/vec4 v0xcbce00_0;
    %parti/s 1, 0, 2;
    %inv;
    %load/vec4 v0xcbce00_0;
    %parti/s 1, 1, 2;
    %and;
    %ix/load 4, 2, 0;
    %flag_set/imm 4, 0;
    %store/vec4 v0xcbcf00_0, 4, 1;
    %load/vec4 v0xcbce00_0;
    %parti/s 1, 1, 2;
    %load/vec4 v0xcbce00_0;
    %parti/s 1, 0, 2;
    %and;
    %ix/load 4, 3, 0;
    %flag_set/imm 4, 0;
    %store/vec4 v0xcbcf00_0, 4, 1;
    %jmp T_2;
    .thread T_2, $push;
    .scope S_0xca0710;
T_3 ;
    %vpi_call 2 30 "$dumpfile", "Decode24Test_v.vcd" {0 0 0};
    %vpi_call 2 31 "$dumpvars", 32'sb00000000000000000000000000000000, S_0xca0710 {0 0 0};
    %end;
    .thread T_3;
    .scope S_0xca0710;
T_4 ;
    %pushi/vec4 0, 0, 2;
    %store/vec4 v0xcbd040_0, 0, 2;
    %pushi/vec4 0, 0, 8;
    %store/vec4 v0xcbd200_0, 0, 8;
    %delay 90000, 0;
    %pushi/vec4 0, 0, 2;
    %store/vec4 v0xcbd040_0, 0, 2;
    %delay 10000, 0;
    %load/vec4 v0xcbd130_0;
    %store/vec4 v0xcbc7b0_0, 0, 4;
    %pushi/vec4 1, 0, 4;
    %store/vec4 v0xcbc890_0, 0, 4;
    %pushi/vec4 2463949034, 0, 98;
    %concati/vec4 7610416, 0, 23;
    %store/vec4 v0xcbca60_0, 0, 121;
    %load/vec4 v0xcbd200_0;
    %store/vec4 v0xcbc970_0, 0, 8;
    %fork TD_Decode24Test_v.passTest, S_0xcbc5c0;
    %join;
    %load/vec4 v0xcbc970_0;
    %store/vec4 v0xcbd200_0, 0, 8;
    %delay 90000, 0;
    %pushi/vec4 1, 0, 2;
    %store/vec4 v0xcbd040_0, 0, 2;
    %delay 10000, 0;
    %load/vec4 v0xcbd130_0;
    %store/vec4 v0xcbc7b0_0, 0, 4;
    %pushi/vec4 2, 0, 4;
    %store/vec4 v0xcbc890_0, 0, 4;
    %pushi/vec4 2463949034, 0, 98;
    %concati/vec4 7610417, 0, 23;
    %store/vec4 v0xcbca60_0, 0, 121;
    %load/vec4 v0xcbd200_0;
    %store/vec4 v0xcbc970_0, 0, 8;
    %fork TD_Decode24Test_v.passTest, S_0xcbc5c0;
    %join;
    %load/vec4 v0xcbc970_0;
    %store/vec4 v0xcbd200_0, 0, 8;
    %delay 90000, 0;
    %pushi/vec4 2, 0, 2;
    %store/vec4 v0xcbd040_0, 0, 2;
    %delay 10000, 0;
    %load/vec4 v0xcbd130_0;
    %store/vec4 v0xcbc7b0_0, 0, 4;
    %pushi/vec4 4, 0, 4;
    %store/vec4 v0xcbc890_0, 0, 4;
    %pushi/vec4 2463949034, 0, 98;
    %concati/vec4 7610418, 0, 23;
    %store/vec4 v0xcbca60_0, 0, 121;
    %load/vec4 v0xcbd200_0;
    %store/vec4 v0xcbc970_0, 0, 8;
    %fork TD_Decode24Test_v.passTest, S_0xcbc5c0;
    %join;
    %load/vec4 v0xcbc970_0;
    %store/vec4 v0xcbd200_0, 0, 8;
    %delay 90000, 0;
    %pushi/vec4 3, 0, 2;
    %store/vec4 v0xcbd040_0, 0, 2;
    %delay 10000, 0;
    %load/vec4 v0xcbd130_0;
    %store/vec4 v0xcbc7b0_0, 0, 4;
    %pushi/vec4 8, 0, 4;
    %store/vec4 v0xcbc890_0, 0, 4;
    %pushi/vec4 2463949034, 0, 98;
    %concati/vec4 7610419, 0, 23;
    %store/vec4 v0xcbca60_0, 0, 121;
    %load/vec4 v0xcbd200_0;
    %store/vec4 v0xcbc970_0, 0, 8;
    %fork TD_Decode24Test_v.passTest, S_0xcbc5c0;
    %join;
    %load/vec4 v0xcbc970_0;
    %store/vec4 v0xcbd200_0, 0, 8;
    %delay 90000, 0;
    %load/vec4 v0xcbd200_0;
    %store/vec4 v0xcbc4e0_0, 0, 8;
    %pushi/vec4 4, 0, 8;
    %store/vec4 v0xca2860_0, 0, 8;
    %fork TD_Decode24Test_v.allPassed, S_0xca0890;
