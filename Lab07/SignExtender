    %join;
    %load/vec4 v0xa3c460_0;
    %store/vec4 v0xa3cfd0_0, 0, 8;
    %delay 90, 0;
    %pushi/vec4 1, 0, 2;
    %store/vec4 v0xa3ce00_0, 0, 2;
    %delay 10, 0;
    %load/vec4 v0xa3cd10_0;
