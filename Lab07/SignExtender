#! /usr/bin/vvp
:ivl_version "10.2 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "system";
:vpi_module "vhdl_sys";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_0xa1b670 .scope module, "SignExtenderTest" "SignExtenderTest" 2 2;
 .timescale 0 0;
v0xa3cd10_0 .net "BusImm", 63 0, v0xa3c900_0;  1 drivers
v0xa3ce00_0 .var "Ctrl", 1 0;
v0xa3ced0_0 .var "Inst26", 25 0;
v0xa3cfd0_0 .var "passed", 7 0;
S_0xa1b7f0 .scope task, "allPassed" "allPassed" 2 18, 2 18 0, S_0xa1b670;
 .timescale 0 0;
v0xa1b9c0_0 .var "numTests", 7 0;
v0xa3bfd0_0 .var "passed", 7 0;
TD_SignExtenderTest.allPassed ;
    %load/vec4 v0xa3bfd0_0;
    %load/vec4 v0xa1b9c0_0;
    %cmp/e;
    %jmp/0xz  T_0.0, 4;
    %vpi_call 2 22 "$display", "All tests passed" {0 0 0};
    %jmp T_0.1;
T_0.0 ;
    %vpi_call 2 23 "$display", "Some tests failed" {0 0 0};
T_0.1 ;
    %end;
S_0xa3c0b0 .scope task, "passTest" "passTest" 2 9, 2 9 0, S_0xa1b670;
 .timescale 0 0;
v0xa3c2a0_0 .var "actualOut", 63 0;
v0xa3c380_0 .var "expectedOut", 63 0;
v0xa3c460_0 .var "passed", 7 0;
v0xa3c550_0 .var "testType", 120 0;
TD_SignExtenderTest.passTest ;
    %load/vec4 v0xa3c2a0_0;
    %load/vec4 v0xa3c380_0;
    %cmp/e;
    %jmp/0xz  T_1.2, 4;
    %vpi_call 2 14 "$display", "%s passed", v0xa3c550_0 {0 0 0};
    %load/vec4 v0xa3c460_0;
    %addi 1, 0, 8;
    %store/vec4 v0xa3c460_0, 0, 8;
    %jmp T_1.3;
T_1.2 ;
    %vpi_call 2 15 "$display", "%s failed: %d should be %d", v0xa3c550_0, v0xa3c2a0_0, v0xa3c380_0 {0 0 0};
T_1.3 ;
    %end;
S_0xa3c630 .scope module, "uut" "SignExtender" 2 35, 3 1 0, S_0xa1b670;
 .timescale 0 0;
    .port_info 0 /OUTPUT 64 "BusImm"
    .port_info 1 /INPUT 26 "Inst26"
    .port_info 2 /INPUT 2 "Ctrl"
v0xa3c900_0 .var "BusImm", 63 0;
v0xa3ca00_0 .net "Ctrl", 1 0, v0xa3ce00_0;  1 drivers
v0xa3cae0_0 .net "Inst26", 25 0, v0xa3ced0_0;  1 drivers
v0xa3cbd0_0 .var "extBit", 0 0;
E_0xa3c880 .event edge, v0xa3ca00_0, v0xa3cae0_0, v0xa3cbd0_0;
    .scope S_0xa3c630;
T_2 ;
    %wait E_0xa3c880;
    %load/vec4 v0xa3ca00_0;
    %cmpi/e 0, 0, 2;
    %jmp/0xz  T_2.0, 4;
    %load/vec4 v0xa3cae0_0;
    %parti/s 1, 21, 6;
    %store/vec4 v0xa3cbd0_0, 0, 1;
    %load/vec4 v0xa3cbd0_0;
    %replicate 52;
    %load/vec4 v0xa3cae0_0;
    %parti/s 12, 10, 5;
    %concat/vec4; draw_concat_vec4
    %store/vec4 v0xa3c900_0, 0, 64;
    %jmp T_2.1;
T_2.0 ;
    %load/vec4 v0xa3ca00_0;
    %cmpi/e 1, 0, 2;
    %jmp/0xz  T_2.2, 4;
    %load/vec4 v0xa3cae0_0;
    %parti/s 1, 20, 6;
    %store/vec4 v0xa3cbd0_0, 0, 1;
    %load/vec4 v0xa3cbd0_0;
    %replicate 55;
    %load/vec4 v0xa3cae0_0;
    %parti/s 9, 12, 5;
    %concat/vec4; draw_concat_vec4
    %store/vec4 v0xa3c900_0, 0, 64;
    %jmp T_2.3;
T_2.2 ;
    %load/vec4 v0xa3ca00_0;
    %cmpi/e 2, 0, 2;
    %jmp/0xz  T_2.4, 4;
    %load/vec4 v0xa3cae0_0;
    %parti/s 1, 25, 6;
    %store/vec4 v0xa3cbd0_0, 0, 1;
    %load/vec4 v0xa3cbd0_0;
    %replicate 38;
    %load/vec4 v0xa3cae0_0;
    %concat/vec4; draw_concat_vec4
    %store/vec4 v0xa3c900_0, 0, 64;
    %jmp T_2.5;
T_2.4 ;
    %load/vec4 v0xa3ca00_0;
    %cmpi/e 3, 0, 2;
    %jmp/0xz  T_2.6, 4;
    %load/vec4 v0xa3cae0_0;
    %parti/s 1, 23, 6;
    %store/vec4 v0xa3cbd0_0, 0, 1;
    %load/vec4 v0xa3cbd0_0;
    %replicate 45;
    %load/vec4 v0xa3cae0_0;
    %parti/s 19, 5, 4;
    %concat/vec4; draw_concat_vec4
    %store/vec4 v0xa3c900_0, 0, 64;
T_2.6 ;
T_2.5 ;
T_2.3 ;
T_2.1 ;
    %jmp T_2;
    .thread T_2, $push;
    .scope S_0xa1b670;
T_3 ;
    %vpi_call 2 6 "$dumpfile", "SignExtenderTest.vcd" {0 0 0};
    %vpi_call 2 7 "$dumpvars", 32'sb00000000000000000000000000000000, S_0xa1b670 {0 0 0};
    %end;
    .thread T_3;
    .scope S_0xa1b670;
T_4 ;
    %pushi/vec4 0, 0, 2;
    %store/vec4 v0xa3ce00_0, 0, 2;
    %pushi/vec4 0, 0, 26;
    %store/vec4 v0xa3ced0_0, 0, 26;
    %pushi/vec4 0, 0, 8;
    %store/vec4 v0xa3cfd0_0, 0, 8;
    %delay 90, 0;
    %pushi/vec4 14, 0, 28;
    %split/vec4 2;
    %store/vec4 v0xa3ce00_0, 0, 2;
    %store/vec4 v0xa3ced0_0, 0, 26;
    %delay 10, 0;
    %load/vec4 v0xa3cd10_0;
    %store/vec4 v0xa3c2a0_0, 0, 64;
    %pushi/vec4 3, 0, 64;
    %store/vec4 v0xa3c380_0, 0, 64;
    %pushi/vec4 3292588274, 0, 58;
    %concati/vec4 3771351250, 0, 32;
    %concati/vec4 1852863860, 0, 31;
    %store/vec4 v0xa3c550_0, 0, 121;
    %load/vec4 v0xa3cfd0_0;
    %store/vec4 v0xa3c460_0, 0, 8;
    %fork TD_SignExtenderTest.passTest, S_0xa3c0b0;
    %join;
    %load/vec4 v0xa3c460_0;
    %store/vec4 v0xa3cfd0_0, 0, 8;
    %delay 90, 0;
    %pushi/vec4 0, 0, 2;
    %store/vec4 v0xa3ce00_0, 0, 2;
    %delay 10, 0;
    %load/vec4 v0xa3cd10_0;
    %store/vec4 v0xa3c2a0_0, 0, 64;
    %load/vec4 v0xa3ced0_0;
    %parti/s 1, 21, 6;
    %replicate 52;
    %pad/u 64;
    %load/vec4 v0xa3ced0_0;
    %parti/s 12, 10, 5;
    %pad/u 64;
    %add;
    %store/vec4 v0xa3c380_0, 0, 64;
    %pushi/vec4 3527469298, 0, 58;
    %concati/vec4 3771351250, 0, 32;
    %concati/vec4 1852863860, 0, 31;
    %store/vec4 v0xa3c550_0, 0, 121;
